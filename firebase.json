{
	"hosting": {
		"public": "output/dist",
		"headers": [
			{
				"source" : "**/*.@(svg|css|js|jpg|jpeg|gif|png)",
				"headers" : [
					{
						"key" : "Cache-Control",
						"value" : "max-age=30672000"
					}
				]
			},
			{
				"source" : "**/service-worker.js",
				"headers" : [
					{
						"key" : "Cache-Control",
						"value" : "max-age=0"
					}
				]
			},
			{
				"source" : "**/workbox-*/*.js",
				"headers" : [
					{
						"key" : "Cache-Control",
						"value" : "max-age=0"
					}
				]
			}
		],
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "/hn/**", "function": "hn"
			}
		]
	}
}
