{
	"hosting": {
		"public": "output/dist",
		"headers": [
			{
				"source": "/",
				"headers": [
					{
						"key": "Link",
						"value": "</main.6da8d1e07a8cfff3cce8.bundle.js>;rel=preload;as=script,</main.b70a48f2f7a2e16634818e22932b2ef3.bundle.css>;rel=preload;as=style,</service-worker.js>;rel=preload;as=script,</favicon.ico>;rel=preload;as=image,</hn/news?page=1&offset=0>;rel=preload;as=fetch"
					}
				]
			},
			{
				"source": "**/*.@(svg|css|js|jpg|jpeg|gif|png)",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "max-age=30672000"
					}
				]
			},
			{
				"source": "**/service-worker.js",
				"headers": [
					{
						"key": "Cache-Control",
