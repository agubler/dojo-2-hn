{
  "build-webpack": {
        "pwa": {
            "manifest": {
                "name": "dojo-2-hn",
                "short_name": "dojo-2-hn",
                "description": "dojo-2-hn",
                "background_color": "#ffffff",
                "theme_color": "#222127",
                "icons": [
                    {
                          "src": "src/img/hn.png",
                          "sizes": [ 128, 256, 512 ]
                    }
                ]
            }
        },
        "buildTimeRender": {
			"root": "app",
			"paths": [
				{
					"path": "#comments/9999",
					"match": [ "#comments\/.*" ]
				},
				{
					"path": "#dummy/1",
					"match": [ "#[a-zA-Z]+\/\\d+" ]
				}
			]
        }
  },
  "test-intern": {
  }
}
