{
  "build-webpack": {
        "pwa": {
            "manifest": {
                "name": "dojo-2-hn",
                "short_name": "dojo-2-hn",
                "description": "dojo-2-hn",
                "background_color": "#ffffff",
                "theme_color": "#222127",
                "icons": [
                    {
                          "src": "src/img/hn.png",
                          "sizes": [ 128, 256, 512 ]
                    }
                ]
            },
            "serviceWorker": {
                "caches": {
                    "main": [ "index.html", "**/*.js", "**/*.css", "**/*.svg" ],
                    "optional": [ ":rest:" ]
                },
                "request": [
                    { "method": "get", "path": "/*",  "origin": "https://api.hackerwebapp.com", "strategy": "cacheFirst" }
                ]
            }
        }
  },
  "test-intern": {
  }
}
